#include <iostream>

int NOD(int a, int b)
{
    while (a > 0 && b > 0)
    {
        if (a > b)
        {
            a %= b;
        }
        else if(b > a)
        {
            b %= a;
        }
        else if(a == 0 || b == 0)
        {
            a = 0;
            b = 0;
        }
        else if(a == b)
        {
            a = 0;
        }
    }
    
    return a + b;
}

int main()
{
    int a, b;
    std::cin >> a >> b;
    std::cout << NOD(a, b);
}
